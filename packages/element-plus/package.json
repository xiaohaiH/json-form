{
    "name": "@xiaohaih/json-form-plus",
    "type": "module",
    "version": "0.2.6",
    "description": "基于element-plus实现, 通过JSON配置表单组件(表单项间可相互依赖)",
    "author": "xiaohai",
    "license": "MIT",
    "homepage": "https://xiaohaih.github.io/json-form/example-plus-vue3/index.html",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xiaohaiH/json-form/tree/master/packages/element-plus"
    },
    "keywords": [
        "vue3",
        "element-plus",
        "JSON Form"
    ],
    "exports": {
        ".": {
            "browser": "./src/index.ts",
            "types": "./src/index.ts",
            "import": "./src/index.ts",
            "require": "./dist/index.cjs.js"
        },
        "./*": "./*"
    },
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "unpkg": "dist/index.umd.js",
    "jsdelivr": "dist/index.umd.js",
    "types": "src/index.ts",
    "scripts": {
        "build": "node ./scripts/generate-version.mjs && vite build",
        "build:dts": "tsdown",
        "postinstall": "node -e \"try{require('./scripts/postinstall.cjs')}catch(e){}\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs"
    },
    "peerDependencies": {
        "element-plus": "^2.0.0",
        "vue": "^3.0.0"
    },
    "dependencies": {
        "@xiaohaih/json-form-core": "workspace:*"
    },
    "devDependencies": {
        "@docsearch/js": "4",
        "@popperjs/core": "^2.11.8",
        "@rollup/plugin-terser": "^0.4.4",
        "@tsconfig/node22": "^22.0.0",
        "@vitejs/plugin-vue": "^5.2.1",
        "@vitejs/plugin-vue-jsx": "^4.1.1",
        "element-plus": "^2.10.6",
        "tsdown": "^0.14.0",
        "unocss": "^66.5.0",
        "unplugin-vue": "^7.0.1",
        "vite": "^6.1.0",
        "vite-plugin-dts": "^4.5.3",
        "vitepress": "2.0.0-alpha.12",
        "vue": "^3.5.13",
        "vue-component-type-helpers": "^2.1.10",
        "vue-tsc": "^3.0.5"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "main": "dist/index.cjs.js",
        "module": "dist/index.esm.js",
        "unpkg": "dist/index.umd.js",
        "jsdelivr": "dist/index.umd.js",
        "types": "src/index.ts",
        "exports": {
            ".": {
                "browser": "./src/index.ts",
                "types": "./src/index.ts",
                "import": "./src/index.ts",
                "require": "./dist/index.cjs.js"
            },
            "./*": "./*"
        }
    }
}
